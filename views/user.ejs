<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="/stylesheets/styleUser.css" />
  </head>
  <body>
    <ul>
      <% users.forEach((user) => { %>
      <li><%= user.name %></li>
      <li><%= user.email %></li>
      <li><%= user.password %></li>
      <button id="<%=user.id %>" data-action="remove">X</button>
      <% }) %>
    </ul>

    <script src="/javascripts/index.js"></script>
  </body>
</html>
